{
  "name": "vite-rollup-plugins",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Matias Capeletto",
  "description": "A playground for Vite compatible official rollup plugins",
  "packageManager": "pnpm@7.9.0",
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "scripts": {
    "copy": "shx mkdir -p public/shiki && shx cp -r node_modules/shiki/{dist,languages,samples,themes} public/shiki/",
    "prepare": "pnpm run copy",
    "dev": "vite --open",
    "build": "vite build",
    "preview": "vite preview --open",
    "lint": "eslint ."
  },
  "dependencies": {
    "@antfu/utils": "^0.5.2",
    "@vueuse/core": "^10.7.2",
    "fuse.js": "^6.6.2",
    "htm": "^3.1.1",
    "nanoid": "^5.0.5",
    "shiki": "^0.11.1",
    "vue": "^3.4.18"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/plugin-proposal-pipeline-operator": "^7.18.9",
    "@iconify/json": "^2.2.182",
    "@rollup/plugin-auto-install": "^3.0.5",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-beep": "^1.0.3",
    "@rollup/plugin-dsv": "^3.0.4",
    "@rollup/plugin-eslint": "^9.0.5",
    "@rollup/plugin-graphql": "^2.0.4",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-inject": "^5.0.5",
    "@rollup/plugin-legacy": "^3.0.2",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-strip": "^3.0.4",
    "@rollup/plugin-virtual": "^3.0.2",
    "@rollup/plugin-yaml": "^4.1.2",
    "@types/node": "^20.11.17",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/compiler-sfc": "^3.4.18",
    "babel-plugin-htm": "^3.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.21.1",
    "graphql": "^16.8.1",
    "pnpm": "^8.15.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-sizes": "^1.0.6",
    "rollup-plugin-visualizer": "^5.12.0",
    "shx": "^0.3.4",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-icons": "^0.18.5",
